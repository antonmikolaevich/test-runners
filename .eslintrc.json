{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc" : 0,
        "valid-jsdoc" : 0,
        "max-len": [
            "error",
            {
              "code": 100,
              "tabWidth": 2,
              "ignoreComments": true, 
              "ignoreUrls": true,
              "ignoreStrings": true,
              "ignoreTemplateLiterals": true
            }
          ]
    }
}
